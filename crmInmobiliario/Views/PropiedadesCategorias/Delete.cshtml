@model crmInmobiliario.Models.PropiedadesCategoria

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar Categoría de Propiedad</h2>


<div class="alert alert-danger" role="alert">
    <strong>ATENCIÓN!!!</strong> <h3>Esta seguro(a) de Eliminar esta Categoría de Propiedad?</h3>
</div>

<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Categoria)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Categoria)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Clave)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Clave)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Activo)
        </dt>

        <dd>
            @if (@Model.Activo.Value)
            {
                <i class="fa fa-check-circle text-success text-center"></i>
            }
            else
            {
                <i class="fa fa-times-circle text-danger text-center"></i>
            }
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <button class="btn btn-primary" onclick="location.href='@Url.Action("Index", "PropiedadesCategorias")';return false;"><i class="fa fa-chevron-left"></i> Volver</button>
            <button class="btn btn-danger" type="submit"><i class="fa fa-trash"></i> Eliminar</button>
        </div>
    }
</div>
