@model crmInmobiliario.Models.AspNetUsers

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar Usuario</h2>


<div class="alert alert-danger" role="alert">
    <strong>ATENCIÓN !!!</strong> <h3>Esta seguro(a) de Eliminar este Usuario?</h3>
</div>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Email)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>


        <dt>
            @Html.DisplayNameFor(model => model.UserName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.UserName)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <button class="btn btn-primary" onclick="location.href='@Url.Action("Index", "AspNetUsers")';return false;"><i class="fa fa-chevron-left"></i> Volver</button>
                <button class="btn btn-danger" type="submit"><i class="fa fa-trash"></i> Eliminar</button>
            </div>
    }
</div>
