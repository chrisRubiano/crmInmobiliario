@model IEnumerable<crmInmobiliario.Models.ProspectosIncidencias>

@{
    ViewBag.Title = "Incidencias de clientes/prospectos";
}

<h2>Incidencias de clientes/prospectos</h2>
<h4>Estos clientes o prospectos intentaron ser registrados por un vendedor distinto</h4>

<table class="table">
    <tr>
        <th>
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Prospecto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsuarioRegistro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UsuarioIncidencia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td class="">
            <div class="list-inline">
                <button class="btn btn-danger btn-tblsolc inline" data-toggle="tooltip" data-placement="right" title="Eliminar" onclick="location.href='@Url.Action("Delete", "ProspectosIncidencias", new { id = item.Id })';return false;"><i class="fa fa-fw fa-trash-o"></i></button>
            </div>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Personas.NombreCompleto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetUsers.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetUsers1.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaRegistro)
        </td>
    </tr>
}

</table>
