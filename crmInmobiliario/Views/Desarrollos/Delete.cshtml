@model crmInmobiliario.Models.Desarrollos

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar Desarrollo</h2>


<div class="alert alert-danger" role="alert">
    <strong>Atención!!!</strong> <h3>Esta seguro(a) de Eliminar este Desarrollo?</h3>
</div>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Desarrollo)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Desarrollo)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Clave)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Clave)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Activo)
        </dt>

        <dd>
            @if (@Model.Activo.Value)
            {
                <i class="fa fa-check-circle text-success text-center"></i>
            }
            else
            {
                <i class="fa fa-times-circle text-danger text-center"></i>
            }
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Descuento)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Descuento)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.CajonesEstacionamiento)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CajonesEstacionamiento)
        </dd>


    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        @*<div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>*@

        <div class="form-actions no-color">
            <button class="btn btn-primary" onclick="location.href='@Url.Action("Index", "Desarrollos")';return false;"><i class="fa fa-chevron-left"></i> Volver</button>
            <button class="btn btn-danger" type="submit"><i class="fa fa-trash"></i> Eliminar</button>
        </div>
    }
</div>
