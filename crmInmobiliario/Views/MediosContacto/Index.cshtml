@model IEnumerable<crmInmobiliario.Models.MediosContacto>

@{
    ViewBag.Title = "Index";
}

<h2>Medios de Contacto</h2>


<p>
    <button class="btn btn-success" onclick="location.href='@Url.Action("Create", "MediosContacto")'"><i class="fa fa-user-plus"></i> Registrar Medio</button>
    <button class="btn btn-info" onclick="location.href='@Url.Action("Excel", "MediosContacto")'"><i class="fa fa-file-excel-o"></i> Exportar a Excel</button>
</p>
<table class="table">
    <tr>
        <th></th>
        <th>
            @Html.DisplayNameFor(model => model.MedioContacto)
        </th>
       
    </tr>

@foreach (var item in Model) {
    <tr>
        <td class="">
            <div class="list-inline">
                <button class="btn btn-primary btn-tblsolc inline" data-toggle="tooltip" data-placement="right" title="Editar" onclick="location.href='@Url.Action("Edit", "MediosContacto", new { id = item.IdMedioContacto })';return false;"><i class="fa fa-fw fa-pencil"></i></button>
                <button class="btn btn-success btn-tblsolc inline" data-toggle="tooltip" data-placement="right" title="Detalles" onclick="location.href='@Url.Action("Details", "MediosContacto", new { id = item.IdMedioContacto })';return false;"><i class="fa fa-fw fa-eye"></i></button>
                <button class="btn btn-danger btn-tblsolc inline" data-toggle="tooltip" data-placement="right" title="Eliminar" onclick="location.href='@Url.Action("Delete", "MediosContacto", new { id = item.IdMedioContacto })';return false;"><i class="fa fa-fw fa-trash-o"></i></button>
            </div>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MedioContacto)
        </td>
        
    </tr>
}

</table>
